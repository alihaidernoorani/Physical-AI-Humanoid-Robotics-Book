{
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 401, 403, 429],
  "ignorePatterns": [
    {
      "pattern": "^#"
    },
    {
      "pattern": "^mailto:"
    },
    {
      "pattern": "^tel:"
    },
    {
      "pattern": "^/"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://example.com"
    }
  ]
}